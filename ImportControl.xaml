        <Label Content="Export Data" HorizontalAlignment="Left" Margin="40,131,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="20" FontWeight="Bold"/>
        <Button x:Name="exportStudentsBtn" Content="Export Students" HorizontalAlignment="Left" Margin="40,170,0,0" VerticalAlignment="Top" Width="198" Height="34" FontFamily="Arial" Background="#FF3E206D" Foreground="White" FontWeight="Bold" BorderBrush="{x:Null}" Click="exportStudentsBtn_Click"/>

    </Grid>
</UserControl>
